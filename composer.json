{
    "name": "devzyj/yii2-oauth2-server",
    "description": "OAuth server for Yii2.",
    "keywords": ["yii2", "extension", "oauth", "server"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jerry Zhang",
            "email": "zhangyanjiong@163.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "codeception/codeception": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "devzyj\\oauth2\\server": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "devzyj\\oauth2\\server\\tests\\": "tests/"
        }
    }
}