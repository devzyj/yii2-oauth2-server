{
    "name": "devzyj/yii2-oauth2-server",
    "description": "OAuth server for Yii2.",
    "keywords": ["yii2", "extension", "oauth", "oauth2", "server"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jerry Zhang",
            "email": "zhangyanjiong@163.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*",
        "devzyj/php-oauth2-server": "dev-master"
    },
    "require-dev": {
        "codeception/codeception": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "devzyj\\yii2\\oauth2\\server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "devzyj\\yii2\\oauth2\\server\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/devzyj/php-oauth2-server"
        }
    ]
}